-345                                       ; ↝ 345
=                                       ; ↝ <function>
>                                      ; ↝ <function>
<                                       ; ↝ <function>

(45 (6 -))                               ; ↝ -39
(23 (2 +))                              ; ↝ 25
(99 (99 *))                              ; ↝ 9801
(34 (34 /))                               ; ↝ 1

(0 (lambda (tu) tu))                      ; ↝ 0

(45(3(lambda (x) (lambda (y) (x(y /))))))   ; ↝ 0

(cons yuchen (cons hui 5 3 (45(3(lambda (x) (lambda (y) (x(y /)))))))
                    (cons chen 2 2 3 1 4))                           ; ↝ [yuchen [hui 5 3 0] [chen 2 2 3 1 4]]

(if ((4(5 =))) (34 (34 /)) (0 (lambda (tu) tu)))                      ; ↝ 0
(if ((1 (6 >=))) 1 6)                        ; ↝ 1

(slet ((x (45 (6 -)) )) x)                        ; ↝ -39
